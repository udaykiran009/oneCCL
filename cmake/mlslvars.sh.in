#!/bin/bash

export MLSL_ROOT="@CMAKE_INSTALL_PREFIX@/@MLSL_COMMON_INSTALL_PREFIX@"

if [ -z "${PATH}" ]
then
    PATH="${MLSL_ROOT}/bin"; export PATH
else
    PATH="${MLSL_ROOT}/bin:${PATH}"; export PATH
fi

if [ -z "${LD_LIBRARY_PATH}" ]
then
    LD_LIBRARY_PATH="${MLSL_ROOT}/@CMAKE_INSTALL_LIBDIR@"; export LD_LIBRARY_PATH
else
    LD_LIBRARY_PATH="${MLSL_ROOT}/@CMAKE_INSTALL_LIBDIR@:${LD_LIBRARY_PATH}"; export LD_LIBRARY_PATH
fi

MLSL_ATL_TRANSPORT_PATH="${MLSL_ROOT}/@CMAKE_INSTALL_LIBDIR@"; export MLSL_ATL_TRANSPORT_PATH
FI_PROVIDER_PATH="${MLSL_ROOT}/@CMAKE_INSTALL_LIBDIR@/prov"; export FI_PROVIDER_PATH
